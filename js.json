    "dependencies": {
      "@opam/js_of_ocaml": "*",
      "@opam/js_of_ocaml-compiler": "*",
      "@opam/js_of_ocaml-lwt": "*",
      "http-server": "*",
      "@opam/merlin-extend": "0.4"
    },
    "resolutions": {
      "rebez": "github:jchavarri/rebez#46cbc183",
      "pesy": "0.4.1"
    }
  }
}
